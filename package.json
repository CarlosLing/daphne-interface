{
  "name": "daphne-interface",
  "version": "1.0.0",
  "description": "Daphne unified interface",
  "main": "dist/index.html",
  "scripts": {
    "build": "./node_modules/.bin/webpack --display-optimization-bailout",
    "build:prod": "./node_modules/.bin/webpack -p",
    "watch": "./node_modules/.bin/webpack --watch",
    "serve": "firefox http://localhost:8000/dist/index.html",
    "dist:data": "cp -R src/data dist/assets/",
    "nocache:index": "node node_modules/asset-cache-bust/bin/asset-cache-bust --asset-root dist dist/index.html",
    "watch:data": "onchange \"src/data\" -- npm-run-all dist:data notify-update"
  },
  "author": "SEAK",
  "license": "MIT",
  "dependencies": {
    "annyang": "^2.6.0",
    "bulma": "^0.6.2",
    "d3": "^4.12.2",
    "jquery": "^3.2.1",
    "pubsub-js": "^1.5.8",
    "sortablejs": "^1.7.0",
    "speechkitt": "^1.0.0",
    "vue": "^2.5.13",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "asset-cache-bust": "^2.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-plugin-promise": "^3.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "node-notifier-cli": "^1.1.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.1",
    "vue-loader": "^13.7.0",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0"
  }
}
