{
  "name": "daphne-interface",
  "version": "1.0.0",
  "description": "Daphne unified interface",
  "main": "dist/index.html",
  "scripts": {
    "build": "./node_modules/.bin/webpack --display-optimization-bailout --config webpack.dev.js",
    "build:prod": "./node_modules/.bin/webpack --display-optimization-bailout --config webpack.prod.js",
    "watch": "./node_modules/.bin/webpack --watch --display-optimization-bailout --config webpack.dev.js",
    "serve": "firefox http://localhost:8000/dist/index.html",
    "dist:data": "cp -R src/data dist/assets/",
    "nocache:index": "node node_modules/asset-cache-bust/bin/asset-cache-bust --asset-root dist dist/index.html",
    "watch:data": "onchange \"src/data\" -- npm-run-all dist:data notify-update"
  },
  "author": "SEAK",
  "license": "MIT",
  "dependencies": {
    "annyang": "^2.6.0",
    "bulma": "^0.6.2",
    "d3": "^4.13.0",
    "d3-selection-multi": "^1.0.1",
    "intro.js": "^2.8.0-alpha.1",
    "lodash-es": "^4.17.5",
    "speechkitt": "^1.0.0",
    "vue": "^2.5.13",
    "vuedraggable": "^2.16.0",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "asset-cache-bust": "^2.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-plugin-promise": "^3.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "node-notifier-cli": "^1.1.2",
    "node-sass": "^4.7.2",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "webpack": "^3.10.0",
    "webpack-bundle-analyzer": "^2.10.0",
    "webpack-merge": "^4.1.1"
  }
}
