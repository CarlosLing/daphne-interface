{
  "name": "daphne-interface",
  "version": "1.0.0",
  "description": "Daphne unified interface",
  "main": "dist/index.html",
  "scripts": {
    "build": "./node_modules/.bin/webpack",
    "build:prod": "./node_modules/.bin/webpack -p",
    "watch": "./node_modules/.bin/webpack --watch",
    "serve": "firefox http://localhost:8000/dist/index.html",
    "dist:fonts": "cp -R src/fonts dist/assets/",
    "dist:data": "cp -R src/data dist/assets/",
    "dist:images": "imagemin-newer src/images dist/assets/img -p",
    "nocache:index": "node node_modules/asset-cache-bust/bin/asset-cache-bust --asset-root dist dist/index.html",
    "watch:data": "onchange \"src/data\" -- npm-run-all dist:data notify-update",
    "lint": "npm-run-all --silent --parallel -c lint:*",
    "lint:html": "htmllint dist/*.html",
    "lint:sass": "sass-lint -v -q -c .sass-lint.yml 'src/styles/*.scss'",
    "lint:js": "eslint src/scripts/*.js"
  },
  "author": "SEAK",
  "license": "MIT",
  "dependencies": {
    "bulma": "^0.6.2",
    "jquery": "^3.2.1",
    "pubsub-js": "^1.5.8",
    "sortablejs": "^1.7.0",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "asset-cache-bust": "^2.2.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.8",
    "eslint": "^4.15.0",
    "eslint-plugin-promise": "^3.6.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-loader": "^0.5.4",
    "html-webpack-plugin": "^2.30.1",
    "htmllint": "^0.7.0",
    "htmllint-cli": "0.0.6",
    "imagemin-newer": "^1.0.2",
    "node-notifier-cli": "^1.1.2",
    "node-sass": "^4.7.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.6",
    "script-loader": "^0.7.2",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  }
}
